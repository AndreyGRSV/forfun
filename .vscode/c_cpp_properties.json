{
    "configurations": [
        {
            "name": "Win32 Clang C++26",
            "includePath": [
                "${default}"
            ],
            "configurationProvider": "go2sh.cmake-integration",
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c11",
            "cppStandard": "c++26",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [],
            "compilerArgs": [
                "-std=c++26",
                "-Wall",
                "-Wextra",
                "-Wpedantic"
            ],
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "Linux Clang/GCC C++26",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "compilerPath": "/usr/local/bin/g++-14.2.0",
            "cStandard": "c11",
            "cppStandard": "c++26",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [],
            "compilerArgs": [
                "-std=c++26",
                "-Wall",
                "-Wextra",
                "-Wpedantic"
            ],
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}